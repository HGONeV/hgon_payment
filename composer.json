{
    "name": "hgon/hgon-payment",
    "type": "typo3-cms-extension",
    "description": "Extension for payment services",
    "authors": [
        {
            "name": "Maximilian F\u00e4\u00dfler",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5 || ^10.4 || ^11.5 || ^12.4 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "HGON\\HgonPayment\\": "Classes/"
        }
    },
    "replace": {
        "hgon_payment": "self.version",
        "typo3-ter/hgon-payment": "self.version"
    },
	"extra": {
		"typo3/cms": {
			"extension-key": "hgon_payment"
		}
	}
}